            <registers>
                <register>
                    <name>STREAM</name>
                    <description>Read the XIP stream FIFO (fast bus access to XIP_CTRL_STREAM_FIFO)</description>
                    <addressOffset>0x0</addressOffset>
                    <resetValue>0x00000000</resetValue>
                    <fields>
                        <field>
                            <name>STREAM</name>
